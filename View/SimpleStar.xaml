<UserControl x:Class="NewInvaders.View.SimpleStar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NewInvaders.View"
             mc:Ignorable="d" 
             d:DesignHeight="4" d:DesignWidth="4">
    <Grid>
        <Polygon Points="0,1 4,1 0,4 2,0 4,4" Stroke="Black" x:Name="simpleStar" StrokeThickness="0.3"/>
    </Grid>
</UserControl>
